# Database (for docker compose)
POSTGRES_USER=app
POSTGRES_PASSWORD=app
POSTGRES_DB=tmdb
POSTGRES_PORT=5432

# API
API_PORT=8000

# Embeddings provider choice (you can start with "ollama" then switch to "bedrock")
EMBEDDINGS_PROVIDER=ollama

# Ollama
OLLAMA_BASE_URL=http://ollama:11434
OLLAMA_MODEL=nomic-embed-text

# Bedrock (later)
AWS_REGION=eu-west-1
BEDROCK_EMBED_MODEL_ID=amazon.titan-embed-text-v2:0

# Vector dim (must match your embedding model output dimension)
EMBEDDING_DIM=1024

